 {
    "mappings" : {
      "fluentd" : {
        "properties" : {
          "@timestamp" : {
            "type" : "date",
            "format" : "strict_date_optional_time||epoch_millis"
          },
          "Appid": {
            "type": "string"
          },
          "appCode": {
            "type": "string",
	    "index": "not_analyzed"
          },
          "appId": {
            "type": "string",
	    "index": "not_analyzed"
          },
          "container_id": {
            "type": "string",
	    "index": "not_analyzed"
          },
          "container_name": {
            "type": "string",
            "index": "not_analyzed"
          },
          "level": {
            "type": "string",
            "index": "not_analyzed"
          },
          "log": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "num": {
            "type": "string"
          },
          "source": {
            "type": "string",
	    "index": "not_analyzed"
          },
          "tag": {
            "type": "string",
	    "index": "not_analyzed"
          },
          "time": {
            "type": "string"
          }
        }
      }
    },
    "template": "paas-tomcat-*"
  }
